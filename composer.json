{
    "name": "insly/amqpsender-php-client",
    "description": "A PHP client library for interacting with Amqpsender API",
    "type": "library",
    "require": {
        "php": "^7.4||^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Insly\\AmqpSenderClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Insly\\AmqpSenderClient\\Tests\\": "tests/",
            "Insly\\AmqpSenderClient\\Tests\\Unit\\": "tests/unit"
        }
    },
    "scripts": {
        "test": "vendor/bin/codecept run unit"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0"
    }
}
